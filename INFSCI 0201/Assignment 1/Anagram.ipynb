   "source": [
    "words = open('words.txt')\n",
    "\n",
    "dictionary = [word.lower() for word in words]  \n",
    "\n",
    "user_word = input('Enter a word: ').lower()\n",
    "\n",
    "anagram = True\n",
    "for word in dictionary:\n",
    "    for letter in user_word:\n",
    "        if letter not in word:\n",
    "            anagram = False\n",
    "            break\n",
    "\n",
    "\n"
